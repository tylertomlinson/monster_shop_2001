<h1>Edit Profile</h1>
<center>
  <%= form_tag "/profile/edit_password", method: :patch do %>
    <%= label_tag :password %>
    <%= text_field_tag :password %><br/>

    <%= label_tag :password_confirmation %>
    <%= text_field_tag :password_confirmation %><br/>

    <%= submit_tag "Submit" %>
  <% end %>
</center>
